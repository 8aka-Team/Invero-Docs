{
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cross-env DOCUSAURUS_CURRENT_LOCALE= docusaurus start",
    "start:en": "docusaurus start --locale en",
    "start:zh": "docusaurus start --locale zh-CN",
    "start:multi": "cross-env NODE_ENV=development docusaurus build && docusaurus serve --no-open",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "update": "pnpm update --latest",
    "clean": "pnpm store prune && pnpm install"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/plugin-pwa": "^3.8.1",
    "@docusaurus/plugin-sitemap": "^3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-common": "^3.8.1",
    "@easyops-cn/docusaurus-search-local": "^0.52.1",
    "@giscus/react": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^3.0.1",
    "mitt": "^3.0.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "cross-env": "^10.0.0",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "packageManager": "pnpm@10.14.0"
}
